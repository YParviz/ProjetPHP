{
    "name": "univ-nantes/debate-arena",
    "description": "DebateArena",
    "autoload": {
        "psr-4": {
            "Controllers\\": "src\\app\\Controllers",
            "Entity\\": "src\\app\\Entity",
            "Models\\": "src\\app\\Models",
            "Views\\": "src\\app\\Views",
            "Util\\": "src\\util"
        }
    },
    "authors": [
        {
            "name": "Firmin EON",
            "email": "firmin.eon@etu.univ-nantes.fr"
        }
    ],
    "require": {
        "ext-pdo": "*",
        "fakerphp/faker": "^1.24",
        "nikic/fast-route": "^1.3",
        "php-di/php-di": "^7.0",
        "symfony/dotenv": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.73"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
